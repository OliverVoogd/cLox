// TODO: add ++ and -- operators
class Doughnut {
  cook() {
    print "Dunk in the fryer.";
    this.finish("sprinkles");
  }

  finish(ingredient) {
    print "Finish with " + ingredient;
  }
}

class Cruller < Doughnut {
  finish(ingredient) {
    // No sprinkles, always icing.
    super.finish("icing");
  }
}

var c = Cruller();
var d = Doughnut();
print "Doughnut: ";
d.cook();
print "Cruller: ";
c.cook();